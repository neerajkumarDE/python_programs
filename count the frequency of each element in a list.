nums = [1, 2, 3, 2, 1, 3, 2, 4, 5, 4]
set_num = set(nums)

for i in set_num:
    count=0
    for j in nums:
        if i==j:
            count=count+1
    print(f'{i} matches {count} times')


# via function
def count_freq(num_list):
    count_dict={}
    set_num=set(num_list)
    for i in set_num:
        count=0
        for j in nums:
            if i==j:
                count=count+1
        count_dict[i]=count
    return count_dict
